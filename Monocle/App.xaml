<Application x:Class="Monocle.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Monocle"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="ProgressBarStyle1" TargetType="ProgressBar">
            <!--<Setter Property="Background" Value="{StaticResource ThemeDarkGray2}"/>
            <Setter Property="Foreground" Value="{StaticResource ThemeDarkGray}"/>-->
            <Setter Property="Height" Value="15"/>
            <Setter Property="Maximum" Value="100"/>
            <Setter Property="Minimum" Value="0"/>
        </Style>
        <local:InverseBooleanVisibilityConverter x:Key="InverseBoolToVis"/>
        <local:UploadResultToMessageConverter x:Key="UploadResultToMsg"/>
    </Application.Resources>
</Application>
